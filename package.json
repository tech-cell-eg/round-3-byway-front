{
  "name": "byway_front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx,.json --fix ",
    "stylelint": "stylelint 'src/**/*.{css,scss}' --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run format"
    ],
    "*.{css,scss}": [
      "npm run format",
      "npm run stylelint"
    ]
  },
