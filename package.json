    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.1",
    "react-i18next": "^15.4.1",
    "react-redux": "^9.2.0",
    "react-router": "^7.5.1",
    "react-router-dom": "^7.5.1",
    "react-share": "^5.2.2",
    "react-toastify": "^11.0.5",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "tailwindcss-rtl": "^0.9.0",
    "tw-animate-css": "^1.2.8",
    "zod": "^3.24.3"
